                <Window Type="ArkanaLook/Label" Name="mdlviewer/panel/frmMoves/lblMove">
                    <Property Name="UnifiedPosition"   Value="{{0,015},{0,28}}" />
                    <Property Name="UnifiedSize"       Value="{{0,170},{0,25}}" />
