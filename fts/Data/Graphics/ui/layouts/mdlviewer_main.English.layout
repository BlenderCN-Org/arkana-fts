            <Property Name="UnifiedPosition"    Value="{{0,0000},{1,-40}}" />
            <Property Name="UnifiedSize"        Value="{{1,-500},{0,040}}" />
            <Property Name="UnifiedMinSize"     Value="{{0,0020},{0,035}}" />
