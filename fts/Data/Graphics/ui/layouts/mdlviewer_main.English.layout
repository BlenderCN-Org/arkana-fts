                </Window>
                <Window Type="ArkanaLook/CheckboxLabelLeft" Name="mdlviewer/panel/frmMisc/chkAttPts">
                    <Property Name="UnifiedPosition" Value="{{0,020},{0,120}}" />
                    <Property Name="UnifiedSize"     Value="{{0,198},{0,024}}" />
